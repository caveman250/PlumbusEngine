        {
            "name": "Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/Debug/PlumbusTester/PlumbusTester",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/PlumbusTester",
            "environment": [],
            "externalConsole": false,
            "preLaunchTask": "Build Debug",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "sourceFileMap": {
                "/build/glibc-YYA7BZ/": "/usr/src/glibc"
            }
